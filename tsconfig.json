{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@configs/*": [
        "src/configs/*"
      ],
      "@elements/*": [
        "src/elements/*"
      ],
      "@navigation/*": [
        "src/navigation/*"
      ],
      "@redux/*": [
        "src/redux/*"
      ],
      "@api/*": [
        "src/api/*"
      ]
  }
},
}
